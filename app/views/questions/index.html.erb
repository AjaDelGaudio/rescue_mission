<h1>Questions</h1>

<ul>
  <% @questions.each do |q| %>
    <li><%= q.title %></li>
    <li>
      <%= link_to q.question,
      '/questions/' + q.to_param %>
    </li>
  <%end%>
</ul>

<%= link_to 'Post New Question', new_question_path %>


<% if flash[:notice] %>
   <div class="notice"><%= flash[:notice] %></div>
 <% end %>
